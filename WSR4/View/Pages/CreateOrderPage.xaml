<Page x:Class="WSR4.View.Pages.CreateOrderPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSR4.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="590" d:DesignWidth="1300"
      Title="CreateOrderPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBlock x:Name="Data" Style="{StaticResource ForListView}" />
        <TextBlock x:Name="DeliveryData" Style="{StaticResource ForListView}" />
        <TextBlock x:Name="PicupPoint" Style="{StaticResource ForListView}" />
        <TextBlock x:Name="Composition" Style="{StaticResource ForListView}" />
        <TextBlock x:Name="Code" Style="{StaticResource ForListView}" />

    </Grid>
</Page>
